{
  "firebase": "unitrans",
  "public": "./build/webpack/prod",
  "ignore": [
    "firebase.json",
    "**/node_modules/**",
    "**/.*"
  ],
  "rewrites": [{
    "source": "**",
    "destination": "/index.html"
  }],
  "headers": [{
    "source" : "unitrans*.@(js|map)",
    "headers" : [{
      "key" : "Cache-Control",
      "value" : "max-age=604800, public"
    }]
  },{
    "source" : "*.@(woff|woff2)",
    "headers" : [{
      "key" : "Cache-Control",
      "value" : "max-age=604800, public"
    }]
  }]
}
