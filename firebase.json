{
  "ignore": [
    "firebase.json",
    "**/node_modules/**",
    "**/.*"
  ],
  "headers": [
    {
      "source": "unitrans*.@(js|map)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, public"
        }
      ]
    },
    {
      "source": "*.@(woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, public"
        }
      ]
    }
  ],
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "./build/webpack/prod",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
