{
  "firebase": "unitrans-front",
  "public": ".",
  "ignore": [
    "firebase.json",
    "**/node_modules/**",
    "**/.*"
  ],
  "rewrites": [{
    "source": "**",
    "destination": "/index.html"
  }],
  "headers": [{
    "source" : "**/app/**/*.@(js|map)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=3600"
    }]
  },
  {
    "source" : "**/vendor/**/*.@(js|map)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=7200"
    }]
  },
  {
    "source" : "**/build/**/*.@(js)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=128000"
    }]
  }]
}
